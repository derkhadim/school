<p id="notice"><%= notice %></p>

<h1>Laclasses</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"> Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @laclasses.each do |laclass| %>
      <tr>
        <td><%= laclass.name %></td>
        <td><%= link_to 'Show', laclass %></td>
        <td><%= link_to 'Edit', edit_laclass_path(laclass) %></td>
        <td><%= link_to 'Destroy', laclass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Laclass', new_laclass_path %>
